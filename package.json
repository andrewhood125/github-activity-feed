{
  "name": "github-activity-feed",
  "description": "Get your GitHub public activity feed in a human readable format",
  "main": "src/github-activity-feed.js",
  "scripts": {
    "test": "mocha --require blanket --reporter mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js",
    "coverage": "mocha --require blanket --reporter html-cov > coverage.html && open coverage.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/andrewhood125/github-activity-feed.git"
  },
  "keywords": [
    "GitHub"
  ],
  "author": "Andrew Hood <andrewhood125@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andrewhood125/github-activity-feed/issues"
  },
  "homepage": "https://github.com/andrewhood125/github-activity-feed",
  "devDependencies": {
    "blanket": "^1.1.6",
    "coveralls": "^2.11.2",
    "mocha": "^2.1.0",
    "jquery": "^2.1.3",
    "mocha-lcov-reporter": "0.0.1"
  },
  "config": {
    "blanket": {
      "pattern": "src"
    }
  }
}
